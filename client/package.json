    "apollo-link-token-refresh": "^0.2.6",
    "cookie": "^0.4.0",
    "graphql": "^14.5.7",
    "isomorphic-unfetch": "^3.0.0",
    "jwt-decode": "^2.2.0",
    "next": "9.0.7",
    "next-antd-aza-less": "^1.0.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "tslint": "^5.20.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.2",
    "@graphql-codegen/cli": "^1.7.0",
